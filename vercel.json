{
    "functions": {
        "api/index.py": {
            "runtime": "@vercel/python@latest"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}