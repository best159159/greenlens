{
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/analyze",
            "dest": "/backend/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}